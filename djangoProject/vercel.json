{
      "builds": [{
        "src": "djangoProject/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" ,
          "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoProject/wsgi.py"
        }
    ]
}



